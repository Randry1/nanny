<%= form_for(nursemaid)  do |f| %>
    <div class="form-group">
      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>
    </div>
    <div class="form-group">
      <%= f.label :education %>
      <%= f.text_field :education, class: 'form-control' %>
    </div>
    <div class="form-group">
      <%= f.label :experience %>
      <%= f.text_field :experience, class: 'form-control' %>
    </div>
    <div class="form-group">
      <%= f.label :medicine %>
      <%= f.text_field :medicine, class: 'form-control' %>
    </div>
    <div class="form-group">
      <%= f.label :price %>
      <%= f.text_field :price, class: 'form-control' %>
    </div>
    <div class="form-group">
      <%= f.label :video %>
      <%= f.text_field :video, class: 'form-control' %>
    </div>
    <div class="form-group">
      <%= f.submit 'Отправить', class: 'form-control' %>
    </div>
<% end %>